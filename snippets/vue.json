{
  // Place your snippets for vue here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  //   "prefix": "log",
  //   "body": ["console.log('$1');", "$2"],
  //   "description": "Log output to console"
  // }
  "vuenarchy-class-modules": {
    "prefix": "Vuenarchy-Class",
    "body": [
      "<template>",
      "",
      "",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "",
      "@Options({",
      "",
      "})",
      "export default class ${1:example} extends Vue {}",
      "",
      "</script>",
      "<style lang=\"scss\">",
      "",
      "</style>"
    ],
    "description": "Vue Class-Based Template"
  },
  "vuenarchy-setup": {
    "prefix": "Vuenarchy-Setup-Empty",
    "body": [
      "<template>",
      "",
      "",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "",
      "</script>",
      "<style scoped lang=\"scss\">",
      "",
      "</style>"
    ],
    "description": "Vue Setup Template"
  },
  "vuenarchy-setup-body": {
    "prefix": "Vuenarchy-Setup-Body",
    "body": [
      "<template>",
      "  <Body>",
      "    <template v-slot:page-title-icon>",
      "      <fa :icon=\"['fas', 'wand-sparkles']\" />",
      "    </template>",
      "    <template v-slot:page-content>",
      "      <Card>",
      "        <template v-slot:card-title>...</template>",
      "        <template v-slot:card-content> ... </template>",
      "        <template v-slot:card-footer> ...</template>",
      "      </Card>",
      "    </template>",
      "  </Body>",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "import Body from \"../components/Body.vue\";",
      "import Card from \"../components/Card.vue\";",
      "import { ref } from \"vue\";",
      "</script>",
      "<style scoped lang=\"scss\"></style>"
    ],
    "description": "Basic layout with script setup"
  },
  "vuenarchy-card-layout": {
    "prefix": "Vuenarchy-Card-Layout",
    "body": [
      "<Card>",
      "  <template v-slot:card-title>Example Title Slot</template>",
      "  <template v-slot:card-content>Example Content Slot </template>",
      "  <template v-slot:card-footer>Example Footer Slot</template>",
      "</Card>"
    ],
    "description": "Basic Layout Card"
  },
  "vuenarchy-define-props": {
    "prefix": "Vuenarchy-define-props",
    "body": [
      "const props = defineProps({",
      "  message: { type: String, default: \"This is my default value!\" }",
      "});"
    ],
    "description": "Basic Layout Card"
  }
}
